{
  "name": "node-babel",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "dev": "nodemon --exec babel-node -- ./src/bin/www.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "start": "node ./dist/bin/www.js"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "core-js": "^3.37.1",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/eslint-parser": "^7.24.6",
    "@babel/node": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.3.0",
    "nodemon": "^3.1.1"
  }
}
